/*  

x ^ x = 0 â†’ same numbers cancel each other
x ^ 0 = x
in XOR order does not matter

 */



class Solution {
    public int singleNumber(int[] nums) {
        
        int XORed = 0;

        for(int num : nums)
        {
            XORed = XORed^num;
        }

        return XORed;
    }
}
