class Solution {
    public int removeDuplicates(int[] nums) {
        int slow = 2;

        for(int fast=2; fast<nums.length; fast++) // keep first two elemets as it is no matter what
        {
            if(nums[fast] != nums[slow-2])

            {
                nums[slow]=nums[fast];
                slow++;
            }
        }
        return slow;
    }
}
